#include <stdio.h>
#include <stdlib.h>
/*
2 - Faça uma função que simule a leitura de um sensor lendo o valor do teclado
ao final a função retorna este valor
*/

int main() {
  int valor_teclado;
  printf("insira o valor do teclado para leitura: ");
  scanf("%d", &valor_teclado);
  printf("valor lido pelo sensor: %d", valor_teclado);
  return 0;
}
